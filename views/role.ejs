    <script>
        var update,actions,create_buttoms,updates_buttoms,deletes_buttons;
        $.get('/nav', function (recordset) {
            create_buttoms = recordset["recordsets"][0][0]["r_creation"];
            var create_buttomS = JSON.parse(create_buttoms);
            var create_role = create_buttomS.create_role;
            

            if(create_role!='1'){
                // var x = document.getElementById('created__btn_role');
                // x.style.display = "none";
                var elms = document.querySelectorAll("[id='created__btn_role']");

                for(var i = 0; i < elms.length; i++) 
                elms[i].style.display='none';
            }

            updates_buttoms = recordset["recordsets"][0][0]["r_update"];
            var editS_roles = JSON.parse(updates_buttoms);
            var edits_roles = editS_roles.edits_roles;

            if(edits_roles!='1'){
                var elms = document.querySelectorAll("[id='updated__rols']");

                for(var i = 0; i < elms.length; i++) 
                elms[i].style.display='none';
            }

            deletes_buttons = recordset["recordsets"][0][0]["r_delete"];
            var deleteS_roles = JSON.parse(deletes_buttons);
            var delet_roles = deleteS_roles.Deletes_roles;

            if(delet_roles!='1'){
                var elms = document.querySelectorAll("[id='deleted__butn']");

                for(var i = 0; i < elms.length; i++) 
                elms[i].style.display='none';
            }

        })
    </script>
