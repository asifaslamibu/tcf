        function getdata() {
            var f_report = document.getElementById("f_report").value;
            // alert(reg_number)
            var dater = document.getElementById("basicFlatpickr2").value;
            //alert(reg_number)
            if (f_report != "" && dater != "") {
                if (f_report === 'Vehicle') {
                    $.get("/vehicle_monthly?dater=" + dater, function (data, status) {
                        var table = $('#html5-extension').DataTable();
                        table.clear().draw();

                        // table.row.add([reg_number, to, from, data.global_km.toFixed()]).draw().node();
                        // alert("Data: " + data.global_km + "\nStatus: " + status);
                        var len = data.length
                        // alert("hamza "+len)
                        // table.rows().remove().draw();
                        for (var i = 0; i < len; i++) {
                            var region_name = data[i]['region_name'];
                            var area_name = data[i]['area_name'];
                            var reg_number = data[i]['reg_number'];
                            var as_on = data[i]['as_on'];
                            var value = moment(as_on).format('DD/MM/YYYY');
                            var sum_pt_cash = data[i]['sum_pt_cash'];
                            var sum_cng_cash = data[i]['sum_cng_cash'];
                            var sum_de_cash = data[i]['sum_de_cash'];
                            var sum_pt_cc = data[i]['sum_pt_cc'];
                            var sum_cng_cc = data[i]['sum_cng_cc'];
                            var sum_de_cc = data[i]['sum_de_cc'];
                            var total_fuel = data[i]['total_fuel'];
                            // alert(total_fuel);


                            // var rowNode = table.row.add([ 'Fiona White', 32, 'Edinburgh' ]).draw().node();
                            table.row.add([region_name, area_name, reg_number, value, sum_pt_cash, sum_cng_cash, sum_de_cash, sum_pt_cc, sum_cng_cc, sum_de_cc, total_fuel]).draw();
                        }
                        document.getElementById("vehi_month").style.display='block';
                        document.getElementById("region_month").style.display='none';
                        document.getElementById("area_month").style.display='none';
                    });

                }
                else if (f_report === 'Region') {
                    $.get("/region_monthly?dater=" + dater, function (data, status) {
                        
                        var table = $('#html5-extension1').DataTable();
                        table.clear().draw();

                        // table.row.add([reg_number, to, from, data.global_km.toFixed()]).draw().node();
